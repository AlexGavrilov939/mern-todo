{
  "name": "api",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "seed": "ts-node src/seeder.ts",
    "dev": "NODE_ENV=dev ts-node -r tsconfig-paths/register src/server.ts | bunyan -o short",
    "start": "node dist/src/server.js",
    "build": "tsc",
    "prod": "NODE_ENV=prod yarn run build && yarn run start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "prettier --check \"src/**/*.{ts,js}\" && npx eslint",
    "lint-fix": "prettier --write \"**/**/*.{js,ts,json,css,scss,md}\" && npx eslint '**/**/*.{js,ts}' --fix",
    "lint-fix2": "prettier --write \"src/**/*.{ts,js}\" && npx eslint --fix"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "bunyan": "^1.8.15",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "est": "^0.2.1-alpha",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
